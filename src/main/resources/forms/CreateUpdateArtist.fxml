<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.db.courseproject.musicstore.controller.CreateUpdateArtistController"
            maxHeight="155.0" maxWidth="260.0" minHeight="155.0" minWidth="260.0" prefHeight="155.0" prefWidth="260.0">
    <children>
        <VBox maxHeight="1545.0" maxWidth="260.0" minHeight="155.0" minWidth="260.0" prefHeight="155.0" prefWidth="260.0">
            <children>
                <HBox maxWidth="250.0" minWidth="250.0" prefHeight="25.0" prefWidth="250.0">
                    <children>
                        <Label maxWidth="100.0" minWidth="100.0" prefWidth="100.0" text="Id*:" />
                        <TextField fx:id="tfId" minWidth="150.0" prefWidth="150.0" />
                    </children>
                    <VBox.margin>
                        <Insets left="5.0" top="5.0" />
                    </VBox.margin>
                </HBox>
                <HBox maxWidth="250.0" minWidth="250.0" prefHeight="25.0" prefWidth="250.0">
                    <children>
                        <Label maxWidth="100.0" minWidth="100.0" prefWidth="100.0" text="First name*:" />
                        <TextField fx:id="tfFirstName" minWidth="150.0" prefWidth="150.0" />
                    </children>
                    <VBox.margin>
                        <Insets left="5.0" top="5.0" />
                    </VBox.margin>
                </HBox>
                <HBox maxWidth="250.0" minWidth="250.0" prefHeight="25.0" prefWidth="250.0">
                    <children>
                        <Label maxWidth="100.0" minWidth="100.0" prefWidth="100.0" text="Last name*:" />
                        <TextField fx:id="tfLastName" minWidth="150.0" prefWidth="150.0" />
                    </children>
                    <VBox.margin>
                        <Insets left="5.0" top="5.0" />
                    </VBox.margin>
                </HBox>
                <HBox maxWidth="250.0" minWidth="250.0" prefHeight="25.0" prefWidth="250.0">
                    <children>
                        <Label maxWidth="100.0" minWidth="100.0" prefWidth="100.0" text="Birth date:" />
                        <TextField fx:id="tfBirthDate" minWidth="150.0" prefWidth="150.0" />
                    </children>
                    <VBox.margin>
                        <Insets left="5.0" top="5.0" />
                    </VBox.margin>
                </HBox>
                <HBox maxWidth="250.0" minWidth="250.0" prefHeight="100.0" prefWidth="250.0">
                    <children>
                        <Button fx:id="btConfirm" mnemonicParsing="false" prefWidth="100.0" onAction="#btConfirmClick" text="Confirm" />
                        <Button fx:id="btCancel" mnemonicParsing="false" prefWidth="100.0" onAction="#btCancelClick" text="Cancel">
                            <HBox.margin>
                                <Insets left="50.0" />
                            </HBox.margin></Button>
                    </children>
                    <VBox.margin>
                        <Insets left="5.0" top="5.0" />
                    </VBox.margin>
                </HBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
